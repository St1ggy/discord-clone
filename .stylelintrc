{
  "extends": "stylelint-config-recommended-scss",
  "rules": {
    "declaration-block-trailing-semicolon": null,
    "no-descending-specificity": null,
    "at-rule-no-unknown": null,
    "max-empty-lines": 1,
    "scss/at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind"
        ]
      }
    ]
  }
}
